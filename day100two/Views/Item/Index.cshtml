@model day100two.ViewModel.UserClientViewModel
@{
    ViewData["Title"] = "Index";
}

<h1>@Model.item.itemName</h1>
<h1>@Model.user.userName</h1>

@foreach (var itemo in Model.Items)
{
    <li><a asp-action="Index" asp-route-sortOder="ViewBag.itemParam">@itemo.itemName</a></li>
}
<hr />


<table border="1">

   


    <tr>
        <th>Item name</th>
    </tr>

    @foreach (var item in Model.Items)
    {
<tr>

    <td>@item.itemName</td>

</tr>
    }

    </table>


@model WebApplication7.ViewModel.UserItemViewModel

<h1>@Model.user.userName</h1>
<hr />
@foreach (var item in Model.items)
{
    <h1>@item.itemName</h1>

}
<br />
<br />

<table border="1">
    @foreach (var tableHeader in Model.items)
    {
        <tr>
            <th>@tableHeader.itemName</th>
        </tr>

    }
   
</table>
